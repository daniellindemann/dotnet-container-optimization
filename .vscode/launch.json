{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "01-dotnet-config",
            "type": "coreclr",
            "request": "launch",
            "launchSettingsProfile": "http",
            "preLaunchTask": "01-dotnet-config-build",
            "program": "${workspaceFolder}/src/01-dotnet-config/bin/Debug/net7.0/01-dotnet-config.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/01-dotnet-config",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "logging": {
                "moduleLoad": false
            },
            "presentation": {
                "hidden": false,
                "group": "01-dotnet-config",
                "order": 10
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}